<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="736.0" prefWidth="499.0" styleClass="hbox" stylesheets="@../../CSS/Background.css"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="jedzonko.controller.common.Register">
   <Label layoutX="30.0" layoutY="14.0" prefHeight="76.0" prefWidth="368.0" stylesheets="@../../CSS/Label.css" text="Stwórz nowe konto:"/>
   <Button layoutX="237.0" layoutY="674.0" mnemonicParsing="false" onAction="#register" prefHeight="54.0"
           prefWidth="205.0" stylesheets="@../../CSS/Button.css" text="Zarejestruj się"/>
   <Button layoutX="30.0" layoutY="674.0" mnemonicParsing="false" onAction="#changeSceneToWelcome" prefHeight="54.0"
           prefWidth="199.0" stylesheets="@../../CSS/Button.css" text="Wróć"/>
   <TextField fx:id="name" layoutX="30.0" layoutY="106.0" prefHeight="47.0" prefWidth="406.0" promptText="Imię"
              stylesheets="@../../CSS/TextField.css"/>
   <TextField fx:id="surname" layoutX="30.0" layoutY="164.0" prefHeight="47.0" prefWidth="406.0" promptText="Nazwisko"
              stylesheets="@../../CSS/TextField.css"/>
   <TextField fx:id="email" layoutX="30.0" layoutY="218.0" prefHeight="47.0" prefWidth="406.0" promptText="Email"
              stylesheets="@../../CSS/TextField.css"/>
   <TextField fx:id="phoneNumber" layoutX="30.0" layoutY="273.0" prefHeight="47.0" prefWidth="406.0"
              promptText="Nr telefonu" stylesheets="@../../CSS/TextField.css"/>
   <TextField fx:id="street" layoutX="30.0" layoutY="329.0" prefHeight="47.0" prefWidth="406.0" promptText="Ulica"
              stylesheets="@../../CSS/TextField.css"/>
   <TextField fx:id="postalCode" layoutX="30.0" layoutY="384.0" prefHeight="47.0" prefWidth="216.0"
              promptText="Kod pocztowy" stylesheets="@../../CSS/TextField.css"/>
   <TextField fx:id="city" layoutX="30.0" layoutY="439.0" prefHeight="47.0" prefWidth="406.0" promptText="Miasto"
              stylesheets="@../../CSS/TextField.css"/>
   <TextField fx:id="houseNumber" layoutX="30.0" layoutY="496.0" prefHeight="47.0" prefWidth="142.0"
              promptText="Nr domu" stylesheets="@../../CSS/TextField.css"/>
   <TextField fx:id="login" layoutX="30.0" layoutY="554.0" prefHeight="47.0" prefWidth="406.0" promptText="Login"
              stylesheets="@../../CSS/TextField.css"/>
   <PasswordField fx:id="password" layoutX="30.0" layoutY="609.0" prefHeight="47.0" prefWidth="406.0" promptText="Hasło"
                  stylesheets="@../../CSS/TextField.css"/>
</AnchorPane>
